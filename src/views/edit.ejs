<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update A User</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>


<body>
    <div class="topnav">
        <a href="/">Home</a>
        <a href="/create">Create Users</a>
    </div>


    <div class="form-add-new">
        <form action="/update-user" method="POST">
            <fieldset>
                <legend>Update new user</legend>
                <div class="input-group d-none">
                    <label>ID :</label>
                    <input type="text" name="userId" value="<%= infUser.id %>" />
                </div>
                <div class="input-group">
                    <label>Email :</label>
                    <input type="text" id="emailId" name="email" value="<%= infUser.email %>" />
                </div>
                <div class="input-group">
                    <label>Name :</label>
                    <input type="text" name="myname" value="<%= infUser.name %>" />
                </div>
                <div class="input-group">
                    <label>City :</label>
                    <input type="text" name="city" value="<%= infUser.city %>" />
                </div>
                <div>
                    <button>Save</button>
                </div>
            </fieldset>
        </form>
    </div>


</body>

</html>